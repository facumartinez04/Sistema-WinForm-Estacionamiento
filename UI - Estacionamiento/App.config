		<add name="ServicesPP" connectionString="Data Source=FACU;Initial Catalog=ServicesPP;Integrated Security=True;Encrypt=False"/>
	</connectionStrings>

	<!-- Configuración de Inicio -->
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
	</startup>


	

