		<add key="MetodoPagoConcreteDAO" value="DAO.Implementations.SqlServer.MetodoPagoDAO"/>
		<add key="FacturaConcreteDAO" value="DAO.Implementations.SqlServer.FacturaDAO"/>
		<add key="EmpleadoConcreteDAO" value="DAO.Implementations.SqlServer.EmpleadoDAO"/>
		<add key="RolConcreteDAO" value="DAO.Implementations.SqlServer.RolDAO"/>

		<add key="LanguagePath" value="I18n/language."/>
